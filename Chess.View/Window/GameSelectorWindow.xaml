<Window x:Class="Chess.View.Window.GameSelectorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Chess.View.Window"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Background="DarkGray" Icon="/Icon/appIcon.png"
        Title="Chess.NET" Height="700" Width="700">
    <DockPanel HorizontalAlignment="Center" LastChildFill="False">
        <Button x:Name="regular" Height="75" Width="200" Margin="10" FontSize="25" FontWeight="Bold" Click="selectorClick">
            Regular Chess
        </Button>
        <Button x:Name="ninesixty" Height="75" Width="200" Margin="10" FontSize="25" FontWeight="Bold" Click="selectorClick">
            960 Chess
        </Button>
    </DockPanel>
</Window>
